target 'Pantry Party' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Pantry Party
  pod 'Firebase'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'Firebase/MLVision'

  target 'Pantry PartyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Pantry PartyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
